<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <context id="DB2Tables" targetRuntime="MyBatis3">
    <commentGenerator>
  <property name="suppressAllComments" value="true" />
  </commentGenerator>
    
    <jdbcConnection 
		driverClass="com.mysql.cj.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/ssyjs_data_system?serverTimezone=UTC&amp;useSSL=false"
        userId="root"
        password="root">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator 
    	targetPackage="com.yjs.bean" 
    	targetProject=".\src\main\java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>

    <sqlMapGenerator 
    targetPackage="mapper"  
    targetProject=".\src\main\resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" 
    targetPackage="com.yjs.dao"  
    targetProject=".\src\main\java">
      <property name="enableSubPackages" value="true" />
    </javaClientGenerator>

    <!--<table tableName="dm_mc" domainObjectName="Dm_mc" ></table>
    <table tableName="province" domainObjectName="Province" ></table>
    <table tableName="school_km" domainObjectName="School_km" ></table>
    <table tableName="school_top" domainObjectName="School_top" ></table>-->
    <table tableName="km_num" domainObjectName="Km_num" ></table>
    <table tableName="zws" domainObjectName="Zws" ></table>
  </context>
</generatorConfiguration>